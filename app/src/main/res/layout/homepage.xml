<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".MainActivity">

    <View
    android:id="@+id/header"
    android:layout_width="match_parent"
    android:layout_height="45dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"/>

    <EditText
        android:id="@+id/editText"
        android:layout_width="343dp"
        android:layout_height="40dp"
        android:background="@drawable/bg_search"
        android:textAlignment="center"
        android:drawableLeft="@drawable/search10"
        android:hint="Search"
        android:inputType="textEmailAddress"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:textColor="#262626"
        android:textColorHint="#262626"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="5dp" />

    <LinearLayout
        android:id="@+id/content1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:orientation="horizontal"
        android:padding="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:visibility="visible"
            app:srcCompat="@drawable/colacan" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="137dp"
            android:layout_height="137dp"
            app:srcCompat="@drawable/kkn" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:visibility="visible"
            app:srcCompat="@drawable/colacan" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/content2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:orientation="horizontal"
        android:padding="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/content1">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:visibility="visible"
            app:srcCompat="@drawable/colacan" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="137dp"
            android:layout_height="137dp"
            app:srcCompat="@drawable/kkn" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:visibility="visible"
            app:srcCompat="@drawable/colacan" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/content3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:orientation="horizontal"
        android:padding="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/content2">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="274dp"
            android:layout_height="274dp"
            android:visibility="visible"
            android:background="@color/black"
            app:srcCompat="@drawable/viewbrown" />

<!--        <ImageView-->
<!--            android:id="@+id/imageView3"-->
<!--            android:layout_width="137dp"-->
<!--            android:layout_height="137dp"-->
<!--            app:srcCompat="@drawable/kkn" />-->

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="137dp"
            android:layout_height="137dp"
            android:visibility="visible"
            app:srcCompat="@drawable/colacan" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/navbar"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="88dp"
        android:orientation="horizontal"

        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/content3">


        <ImageView
            android:id="@+id/ivHome"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:alpha="0.5"
            android:src="@drawable/home" />

        <ImageView
            android:id="@+id/ivSearh"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:alpha="0.5"
            android:src="@drawable/search" />

        <ImageView
            android:id="@+id/ivAdd"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:alpha="0.5"
            android:src="@drawable/add" />

        <ImageView
            android:id="@+id/ivFavorite"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:alpha="0.5"
            android:src="@drawable/chat" />

        <ImageView
            android:id="@+id/ivProfile"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:alpha="0.5"
            android:src="@drawable/instagram" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>